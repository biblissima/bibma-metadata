        <skos:notation rdf:datatype="&xsd;string">P102i</skos:notation>
        <rdfs:domain rdf:resource="&current;E35_Title"/>
        <rdfs:range rdf:resource="&current;E71_Man-Made_Thing"/>
        <rdfs:subPropertyOf rdf:resource="&current;P1i_identifies"/>
    </owl:ObjectProperty>
    
    <!-- http://erlangen-crm.org/current/P106_is_composed_of -->
    
    <owl:ObjectProperty rdf:about="&current;P106_is_composed_of">
        <rdfs:label xml:lang="en">P106 is composed of</rdfs:label>
        <rdfs:label xml:lang="fr">P106 est composé de</rdfs:label>
        <skos:notation rdf:datatype="&xsd;string">P106</skos:notation>
        <rdfs:comment xml:lang="en">Scope note:
            This property associates an instance of E90 Symbolic Object with a part of it that is by itself an instance of E90 Symbolic Object, such as fragments of texts or clippings from an image.
            
            Examples:
            - this Scope note P106 (E33) is composed of fragments of texts (E33)
            - &apos;recognizable&apos; P106 (E90) is composed of &apos;ecognizabl&apos; (E90)</rdfs:comment>
        <rdfs:range rdf:resource="&current;E90_Symbolic_Object"/>
        <rdfs:domain rdf:resource="&current;E90_Symbolic_Object"/>
        <owl:inverseOf rdf:resource="&current;P106i_forms_part_of"/>
    </owl:ObjectProperty>
    
    <!-- http://erlangen-crm.org/current/P106i_forms_part_of -->
    
