<!-- Incunable -->

<rdf:Description rdf:about="[URL_incunable]">
        <rdf:type rdf:resource="http://erlangen-crm.org/efrbroo/F5_Item"/>
        <rdf:type rdf:resource="bibma:Incunabula"/>
            <ecrm:P48_has_preferred_identifier rdf:resource="[URL canonical Shelfmark]"/>
            <bnf-onto:cote>[canonical Shelfmark]</bnf-onto:cote>        
        <ecrm:P102_has_title rdf:resource="[URI Title (Blank Node)]"/>
        <efrbroo:P128_carries rdf:resource="[URL publication expression]"/><!-- répétable --> 
        <ecrm:P50_has_current_keeper rdf:resource="[URL Library]"/>
        <bnfroles:r4010 rdf:resource="[URL former owner]"/><!-- répétable --> 
        <ecrm:P129i_is_subject_of rdf:resource="[URL Bibliographic Reference ]"/>
        <ecrm:P3_has_note>[text string] + [(Source)]</ecrm:P3_has_note>
</rdf:Description>
    
<rdf:Description rdf:about="[URL canonical Shelfmark]">
        <rdf:type rdf:resource="bibma:Shelfmark"/>
        <rdfs:label>[canonical Shelfmark]</rdfs:label>
</rdf:Description>
<rdf:Description rdf:about="[URI Title (Blank Node)]">
        <rdf:type rdf:resource="ecrm:E35_Title"/>
        <rdfs:label>[Title]</rdfs:label>
</rdf:Description>
</rdf:RDF>