<?xml version="1.0" encoding="UTF-8"?>

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:ecrm="http://erlangen-crm.org/current" 
    xmlns:efrbroo="http://erlangen-crm.org/efrbroo"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:owl="https://www.w3.org/TR/owl-guide/"
    xmlns:bibma="https://w3id.org/bibma/"
    xmlns:bnf-onto="http://data.bnf.fr/ontology/bnf-onto/"
    xmlns:bnfroles="http://data.bnf.fr/vocabulary/roles/"
    xmlns:rda="http://rdaregistry.info"
    xmlns:dc="http://dublincore.org/documents/dcmi-namespace/"
    xmlns:bio="xmlns:bio=http://purl.org/vocab/bio/0.1/">
    
<rdf:Description rdf:about="[URL Collection]">
    <rdf:type rdf:resource="current:E78_Collection"/>
    <rdf:type rdf:resource="bibma:Historical_Collection"/>
    <ecrm:P102_has_title rdf:resource="[URI Title (Blank Note)]"/>
    <bnfroles:r4010 rdf:resource="[URL Person or Organisation]"/>
    <!-- <ecrm:P53_has_current_or_former_location rdf:resource="[URL place]"/> -->
    <ecrm:P46_is_composed_of rdf:resource="[URL Book (Manuscript or Incunabula)]"/>
    <ecrm:P70i_is_documented_in rdf:resource="[URL sc:Collection]"/>
    <ecrm:P70i_is_documented_in rdf:resource="[URL notice]"/> <!-- repeatable -->
    <ecrm:P70i_is_documented_in rdf:resource="[URL inventory or sales catalogue]"/> <!-- repeatable -->
    <ecrm:P3_has_note>[Text-String] + [(Source)]</ecrm:P3_has_note>
</rdf:Description>

<rdf:Description rdf:about="[URI Name (Blank Node)]">
    <rdf:type rdf:resource="ecrm:E35_Title ">
        <rdfs:label>[Name]</rdfs:label>
        <rdf:value>[Name]</rdf:value>
    </rdf:type>
</rdf:Description>
    
<rdf:Description rdf:about="[URL sc:Collection]">
    <rdf:type rdf:resource="sc:Collection"/>
</rdf:Description>
<rdf:Description rdf:about="[URL Inventory]">
        <rdf:type rdf:resource="[bibma:Inventory]"/>
</rdf:Description>
    <rdf:Description rdf:about="[URL Record]">
        <rdf:type rdf:resource="[bibma:Record]"/>
    </rdf:Description>

</rdf:RDF>